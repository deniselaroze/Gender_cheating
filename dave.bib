@article{Walter2010,
	Abstract = {The debate on how globalization affects the welfare state has so far$\backslash$nlargely neglected to examine the microlevel causal mechanism underlying$\backslash$ndifferent macrolevel claims. Based on survey data from Switzerland,$\backslash$nthis article provides empirical microfoundations for the so-called$\backslash$ncompensation hypothesis. It finds that globalization losers are more$\backslash$nlikely to express feelings of economic insecurity. Such feelings,$\backslash$nin turn, increase preferences for welfare state expansion, which$\backslash$nin turn increase the likelihood of voting for the Social Democratic$\backslash$nParty. The analysis also shows that globalization losers and winners$\backslash$ndiffer significantly with regard to their social policy preferences$\backslash$nand their propensity to vote for left parties. The article is innovative$\backslash$non two counts: First, it uses a number of different and more nuanced$\backslash$nindicators measuring individuals' positions as beneficiaries or losers$\backslash$nof increasing global competition. Second, rather than focusing on$\backslash$none particular part of the causal chain, it tests the entire individual-level$\backslash$ncausal mechanism implied by the compensation hypothesis.},
	Author = {Walter, Stefanie},
