@article{smith83,
	Abstract = {Attribution theories have not specified whether attributions are made by perceivers as part of the process of comprehending an event or only later in response to specific attributional questions. Theories also disagree about the types of attributional inferences (judgments of causation, of the actor's traits, or of intentionality) that are most likely to be made initially and to mediate further inferences. Whereas previous research has been unable to address these issues, a design using 2 RT measures provided relevant evidence. Results of 2 studies involving 100 undergraduates show that judgments of intention and of the actor's traits may have been made in the process of comprehension; affective judgments and inferences about the repetition of an event and the event's personal or situational causation were probably made later. Implications for a model of schema-based attributional inference are discussed.},
	Author = {Smith, Eliot R. and Miller, Frederick D.},
	Journal = {Journal of Personality and Social Psychology},
	Number = {3},
	Pages = {492-505},
	Title = {Mediation among attributional inferences and comprehension processes: Initial findings and a general method},
