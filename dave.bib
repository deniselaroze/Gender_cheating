@article{Duch2000,
	Abstract = {Empirical findings concerns economic voting differ according to the level of analysis employed.  A widely acceped explanation for the inconsistency between macro- and micro-level evidence of economic voting is the high degree of random variation that plagues survey data.  According to this explanation, aggregation purges individual-level "noise" from mass opinions on policy issues and outcomes.  Building on the research of Bartels (1996), we debate the validity of this explanation by demonstration that public evaluations of the national economy vary systemmatically with information, media exposure, political attitudes, personal experiences, and demographic characteristics.  Furthermore, we show that these sources of subjective heterogeneity produce systematic biases when national economic evaluations are aggregated.  These findings challenge a widely accepted notion that because "error" in individual-level measures (or expressions) of preferences is random, the aggregation of these individual measures ensures that they correctly represent the collective preference.},
	Author = {Duch, Raymond M and Palmer, Harvey D and Anderson, Christopher J},
	Doi = {10.2307/2669272},
	Issn = {0092-5853},
	Journal = {American Journal of Political Science},
	Keywords = {Economic voting,PS 2235,PVS paper,Spring 2009,Voter decisionmaking,economic policy,economic voting,political knowledge,preferences,voting},
	Mendeley-Tags = {PS 2235,PVS paper,Spring 2009,economic policy,economic voting,political knowledge,preferences,voting},
