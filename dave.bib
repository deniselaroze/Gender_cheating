	Eprint = {arXiv:1308.6297v1},
	File = {:Users/akitaka/Dropbox/Mendeley/Library/Mohammad, Turney/Mohammad, Turney - 2011 - Computational Intelligence.pdf:pdf},
	Journal = {Computational Intelligence},
	Keywords = {affect,crowdsourcing,emotion associations,emotion lexicon,emotions,mechanical,polarity,polarity lexicon,semantic orientation,sentiment analysis,turk,word},
	Number = {000},
	Pages = {1--24},
	Title = {{Crowdsourcing a Word -- Emotion Association Lexicon}},
	Url = {http://www.umiacs.umd.edu/~saif/WebDocs/Crowdsourcing-MohammadTurney-CI.pdf},
	Volume = {59},
	Year = {2011},
	Bdsk-Url-1 = {http://www.umiacs.umd.edu/~saif/WebDocs/Crowdsourcing-MohammadTurney-CI.pdf}}

@article{Mohammad2013,
	Abstract = {In this paper, we describe how we created two state-of-the-art SVM classifiers, one to de- tect the sentiment of messages such as tweets and SMS (message-level task) and one to de- tect the sentiment of a term within a message (term-level task). Among submissions from 44 teams in a competition, our submissions stood first in both tasks on tweets, obtaining an F-score of 69.02 in the message-level task and 88.93 in the term-level task. We imple- mented a variety of surface-form, semantic, and sentiment features. We also generated two large word--sentiment association lexi- cons, one from tweets with sentiment-word hashtags, and one from tweets with emoticons. In the message-level task, the lexicon-based features provided a gain of 5 F-score points over all others. Both of our systems can be replicated using freely available resources.},
